<div class="container">
  <%= form_with(model: apply, local: true) do |f| %>
    <div class="form-group">
      <%= f.label :status %>
      <%= f.select :status, Apply.statuses.keys,{}, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= f.submit class: "btn btn-primary"%>
    </div>
  <% end %>
</div>
